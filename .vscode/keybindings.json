// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+alt+,",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+.",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+win+,",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+win+.",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+.",
    "command": "namespaceResolver.expand",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+r",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "ctrl+r",
    "command": "-workbench.action.openRecent"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "alt+n",
    "when": "terminalFocus",
    "command": "workbench.action.terminal.new"
  },
  {
    "key": "alt+e",
    "when": "terminalFocus",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "alt+t",
    "when": "editorFocus",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "alt+shift+e",
    "when": "terminalFocus",
    "command": "workbench.action.terminal.moveToEditor"
  },
  {
    "key": "alt+shift+t",
    "when": "terminalFocus",
    "command": "workbench.action.terminal.moveToTerminalPanel"
  },
  {
    "key": "alt+e", // alt+k in querty layout
    "when": "editorFocus",
    "command": "workbench.action.nextEditorInGroup"
  },
  {
    "key": "alt+n", // alt+j in querty layout
    "when": "editorFocus",
    "command": "workbench.action.previousEditorInGroup"
  },
  {
    "key": "ctrl+alt+e", // ctrl+alt+k in querty layout
    "when": "editorFocus",
    "command": "workbench.action.focusNextGroup"
  },
  {
    "key": "ctrl+alt+n", // ctrl+alt+j in querty layout
    "when": "editorFocus",
    "command": "workbench.action.focusPreviousGroup"
  }
]
